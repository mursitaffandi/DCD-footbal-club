<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mursitaffandi.footballclub">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".schedule_detail.MatchDetailActivity"></activity>
        <activity android:name=".club_detail.TeamDetailActivity"></activity>
        <activity android:name=".club_detail.player_detail.PlayerDetailActivity"></activity>
        <activity android:name=".finding.SearchActivity"></activity>
    </application>

</manifest>